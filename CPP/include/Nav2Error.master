      NAV2ERROR_LINE(  GUIPROT_FAILED_GET_FAVORITE_INFO,         0x0609, qtn_wf_REQ_FAILED_14 )   
      NAV2ERROR_LINE(  GUIPROT_FAILED_ADD_FAVORITE,              0x060a, qtn_wf_REQ_FAILED_15 )   
