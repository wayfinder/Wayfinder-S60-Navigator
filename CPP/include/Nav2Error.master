      NAV2ERROR_LINE(  GUIPROT_FAILED_GET_FAVORITES,             0x0605, qtn_wf_REQ_FAILED_10 )   
      NAV2ERROR_LINE(  GUIPROT_FAILED_GET_FAVORITES_ALL_DATA,    0x0606, qtn_wf_REQ_FAILED_11 )   
